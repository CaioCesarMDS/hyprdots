{
  "group/group-left": {
    "orientation": "inherit",
    "modules": ["custom/notification", "clock", "tray"]
  },
  "custom/notification": {
    "format": "Óöê",
    "tooltip": false,
    "on-click": "swaync-client -t -sw",
    "escape": true
  },
  "clock": {
    "format": "{:%H:%M:%S}",
    "tooltip-format": "<tt>{calendar}</tt>",
    "calendar": {
      "format": {
        "today": "<span color='#fAfBfC'><b>{}</b></span>"
      }
    },
    "interval": 1
  },
  "tray": {
    "icon-size": 14,
    "spacing": 10
  }
}
